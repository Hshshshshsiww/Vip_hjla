local PhantomForcesWindow = Library:NewWindow("hub 3")

local KillingCheats = PhantomForcesWindow:NewSection("Players")

KillingCheats:CreateToggle("Auto bring all", function(value)
    autoRunning = value  -- Set the flag to the toggle's value (true/false)

    if autoRunning then
        -- Auto ON: Start the loop
        while autoRunning do
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

            -- Kho√°¬∫¬£ng cƒÇ¬°ch b√°¬∫¬°n mu√°¬ª‚Äòn √Ñ‚Äò√Ü¬∞a ng√Ü¬∞√°¬ª¬ùi ch√Ü¬°i √Ñ‚Äò√°¬∫¬øn
            local distance = 10

            for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                if otherPlayer ~= player then
                    local otherCharacter = otherPlayer.Character
                    if otherCharacter and otherCharacter:FindFirstChild("HumanoidRootPart") then
                        local targetPosition = humanoidRootPart.Position + (humanoidRootPart.CFrame.LookVector * distance)
                        
                        -- Di chuy√°¬ª∆ín HumanoidRootPart an toƒÇ n
                        otherCharacter.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
                    end
                end
            end

            wait(0)  -- √Ñ¬ê√°¬ª¬£i 1 giƒÇ¬¢y (ho√°¬∫¬∑c √Ñ‚Äòi√°¬ª¬Åu ch√°¬ª‚Ä∞nh √Ñ‚Äò√°¬ª‚Ñ¢ tr√°¬ª‚Ä¶ nh√Ü¬∞ c√°¬∫¬ßn thi√°¬∫¬øt)
        end
    else
        -- Auto OFF: Loop automatically stops when autoRunning is false
        print("Auto mode turned off.")
    end

    print(value)  -- This prints the state of the toggle (true/false)
end)

KillingCheats:CreateTextbox("Hitbox Size players", function(text)
    local newSize = tonumber(text)
    if newSize then
        for _, player in pairs(game.Players:GetPlayers()) do
            -- Exclude your own character
            if player ~= game.Players.LocalPlayer and player.Character then
                local hitbox = player.Character:FindFirstChild("HumanoidRootPart")
                if hitbox then
                    hitbox.Size = Vector3.new(newSize, newSize, newSize)
                    hitbox.Transparency = 0.7  -- Optional: make the hitbox semi-transparent
                    hitbox.CanCollide = false  -- Optional: allow hitbox to pass through objects
                end
            end
        end
    else
        print("Invalid input! Please enter a number.")
    end
end)

KillingCheats:CreateTextbox("Set Speed", function(speedInput)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    
    if humanoid and tonumber(speedInput) then
        humanoid.WalkSpeed = tonumber(speedInput)
        print("Speed set to:", speedInput)
    else
        print("Invalid input or humanoid not found")
    end
end)
-- Creating TextBox for Jump Power
KillingCheats:CreateTextbox("Set Jump Power", function(jumpInput)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    
    if humanoid and tonumber(jumpInput) then
        humanoid.JumpPower = tonumber(jumpInput)
        print("Jump power set to:", jumpInput)
    else
        print("Invalid input or humanoid not found")
    end
end)

